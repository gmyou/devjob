    <!-- jQuery library -->
    <script type="text/javascript" src="http://park7965.cafe24.com/jquery.sliderkit.1.9.2/lib/js/external/jquery-1.6.2.min.js"></script>
    
    <!-- jQuery Plugin scripts -->
    <script type="text/javascript" src="http://park7965.cafe24.com/jquery.sliderkit.1.9.2/lib/js/external/jquery.easing.1.3.min.js"></script>
    <script type="text/javascript" src="http://park7965.cafe24.com/jquery.sliderkit.1.9.2/lib/js/external/jquery.mousewheel.min.js"></script>
    
    <!-- Slider Kit scripts -->
    <script type="text/javascript" src="http://park7965.cafe24.com/jquery.sliderkit.1.9.2/lib/js/sliderkit/jquery.sliderkit.1.9.2.pack.js"></script>

    <!-- Slider Kit launch -->
    <script type="text/javascript">
      $(window).load(function(){ //$(window).load() must be used instead of $(document).ready() because of Webkit compatibility       
        
        
        // Photo slider > Bullets nav
        $(".photoslider-bullets").sliderkit({
          auto:true,
          circular:true,
          mousewheel:true,
          shownavitems:5,
          panelfx:"sliding",
          panelfxspeed:5000,
          panelfxeasing:"easeOutExpo" // "easeOutExpo", "easeInOutExpo", etc.
        });

        
      }); 
    </script>
    
    <!-- Slider Kit styles -->
    <link rel="stylesheet" type="text/css" href="http://park7965.cafe24.com/jquery.sliderkit.1.9.2/lib/css/sliderkit-core.css" media="screen, projection" />
